{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "alerts",
  "info": {
    "name": "Alert"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Reason": {
      "type": "string"
    },
    "OriginBranch": {
      "required": true,
      "type": "integer",
      "min": 1
    },
    "Attachment": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Details": {
      "type": "json"
    },
    "fence_segment": {
      "model": "fence-segment",
      "via": "alert"
    },
    "alert_model": {
      "model": "alert-model",
      "via": "alerts"
    },
    "Errors": {
      "type": "json"
    }
  }
}

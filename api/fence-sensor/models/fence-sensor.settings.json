      "model": "sensor-model"
    },
    "fence_segment": {
      "model": "fence-segment",
      "via": "fence_sensors"
    },
    "SensorName": {
      "type": "string",
      "unique": false,
      "required": false
    },
    "SensorUID": {
      "type": "uid",
      "required": true
    }
  }
}

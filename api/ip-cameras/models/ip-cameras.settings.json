{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "ip_cameras",
  "info": {
    "name": "IPCamera"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "CameraName": {
      "type": "string",
      "unique": false
    },
    "Username": {
      "type": "string",
      "unique": false
    },
    "ip_camera_model": {
      "model": "ip-camera-models",
      "via": "ip_cameras"
    },
    "fence_segment": {
      "model": "fence-segment",
      "via": "ip_cameras"
    },
    "Password": {
      "type": "password"
    },
    "IPCamUID": {
      "type": "uid",
      "required": true
    },
    "Domain": {
      "type": "string"
    }
  }
}

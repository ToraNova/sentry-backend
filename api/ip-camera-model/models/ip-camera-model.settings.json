{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "ip_camera_models",
  "info": {
    "name": "IPCameraModel"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "ModelName": {
      "type": "string"
    },
    "SnapPath": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "GlobalUsername": {
      "type": "string"
    },
    "GlobalPassword": {
      "type": "password",
      "private": true
    },
    "ip_cameras": {
      "collection": "ip-camera",
      "via": "ip_camera_model"
    },
    "StreamPath": {
      "type": "string",
      "required": true
    }
  }
}
